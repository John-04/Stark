# StarkNet SQL Query Sandbox Configuration

# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration
DATABASE_PATH=:memory:
# For persistent storage, use: DATABASE_PATH=./data/starknet.db

# StarkNet RPC Configuration
STARKNET_RPC_URL=https://starknet-mainnet.public.blastapi.io
# Alternative RPC endpoints:
# STARKNET_RPC_URL=https://starknet-mainnet.infura.io/v3/YOUR_PROJECT_ID
# STARKNET_RPC_URL=https://alpha-mainnet.starknet.io

# Indexer Configuration
ENABLE_INDEXER=true
START_BLOCK=100000
BATCH_SIZE=10
SYNC_INTERVAL=30000

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
